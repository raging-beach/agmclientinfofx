<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.MainStageController">
   <children>
      <VBox prefWidth="1200.0">
         <children>
            <MenuBar>
               <menus>
                  <Menu mnemonicParsing="false" text="Info Management">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Clients" />
                        <MenuItem mnemonicParsing="false" onAction="#handleContactManagement" text="Contacts" />
                        <MenuItem mnemonicParsing="false" text="System Users" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="File Management">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Upload/Download" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <AnchorPane fx:id="centerPane" prefHeight="864.0" prefWidth="1200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
